{
    "name": "awia/for-nurseries",
    "type": "project",
    "description": "Nursery Inventory management software",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.3.7",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.4.5",
        "inertiajs/inertia-laravel": "^0.6.3",
        "laravel/framework": "^9.18.0",
        "laravel/jetstream": "^2.9.0",
        "laravel/sanctum": "^2.15.1",
        "laravel/tinker": "^2.7",
        "laraveldaily/laravel-invoices": "^3.0",
        "sammyjo20/saloon": "^1.4",
        "sammyjo20/saloon-laravel": "^1.4",
        "tightenco/ziggy": "^1.4.6"
    },
    "require-dev": {
        "fakerphp/faker": "^1.19.1",
        "jasonmccreary/laravel-test-assertions": "^2.1.1",
        "laracasts/cypress": "2.2.1",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^6.2.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ignition": "^1.3.1",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
